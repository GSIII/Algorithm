n = int(input())
for _ in range(n):
	a = list(map(int,input().split()))[1:]
	b = list(map(int,input().split()))[1:]

	a_cnt = [0] * 5
	b_cnt = [0] * 5

	for num in a:
		a_cnt[num] += 1
	for num in b:
		b_cnt[num] += 1

	winner = 'D'

	for i in range(4,0,-1):
		if a_cnt[i] > b_cnt[i]:
			winner = 'A'
			break
		elif a_cnt[i] < b_cnt[i]:
			winner = 'B'
			break

	print(winner)